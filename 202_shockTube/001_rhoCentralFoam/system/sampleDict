FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      sample;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

type            sets;
libs            (sampling);
interpolationScheme cellPoint;
setFormat       raw;

fields          (U p T rho);

sets
{
    x_sec
    {
        type    uniform;
        axis    x;
        start   (-5 0 0);
        end     (5 0 0);
        nPoints 1000;
    }
}


