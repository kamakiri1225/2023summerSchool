FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    minX
    {
        type            noSlip;
    }
    maxX
    {
        type            noSlip;
    }
    minY
    {
        type            noSlip;
    }
    maxY
    {
        type            noSlip;
    }
    minZ
    {
        type            noSlip;
    }
    maxZ
    {
        type            pressureInletOutletVelocity;
        value           uniform (0 0 0);
    }
    walls
    {
        type            noSlip;
    }
}


